{% extends 'base.html.j2'%}

{% block content %}

    <div class='post'>
        <p>{{ post['created'] }}</p>
        <p>{{post['author']}}</p>
        <p>{{post['category']}}</p>
        <h2>{{ post['title'] }}</h2>
        <div class="blog_post">
            <img src="{{post['img_path']}}" height="250" alt=" "/>
            <p>{{ post['content'] }}</p>
        </div>
        
    </div>
{% endblock %}